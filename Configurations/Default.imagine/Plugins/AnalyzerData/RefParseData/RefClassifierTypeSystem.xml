<bibRefTypeSystem>	<bibRefType name="book">		<constraint require="title"/>		<constraint require="year" pattern="((1[5-9][0-9][0-9])|(20[0-9][0-9]))" message="Year of publication is missing or invalid."/>		<constraint require="journalOrPublisher" message="Publisher and location are missing."/>		<constraint exclude="pagination"/>		<constraint exclude="volume issue numero" message=">Books don't have a volume, issue, or numero."/>	  	<originTemplate>	  	</originTemplate>	  	<modsTemplate>	  	</modsTemplate>	</bibRefType>		<bibRefType name="book chapter">		<constraint require="title"/>		<constraint require="year" pattern="((1[5-9][0-9][0-9])|(20[0-9][0-9]))" message="Year of publication is missing or invalid."/>		<constraint require="journalOrPublisher" message="Publisher and location are missing."/>		<constraint require="pagination" pattern="([1-9][0-9]{0,4})(\s*\-+\s*[1-9][0-9]*)?"/>		<constraint exclude="volume issue numero" message=">Books chapters don't have a volume, issue, or numero."/>	  	<originTemplate>	  	</originTemplate>	  	<modsTemplate>	  	</modsTemplate>	</bibRefType>		<bibRefType name="proceedings">		<constraint require="title"/>		<!--constraint condition="(not(starts-with(./@title, 'Proc')))" message="Volume title does not match proceedings."/-->		<constraint attributes="title" pattern="(((In(\s+(The|the))?)|(in(\s+(The|the))?)|The|the)\s+)?Proc(\.|eedings)\s+.*" message="Title does not match proceedings."/>		<constraint require="year" pattern="((1[5-9][0-9][0-9])|(20[0-9][0-9]))" message="Year of publication is missing or invalid."/>		<constraint exclude="volume issue numero" message=">Proceedings don't have a volume, issue, or numero."/>		<constraint exclude="bookContentHint" message="Proceedings don't have book content hints."/>	  	<originTemplate>	  	</originTemplate>	  	<modsTemplate>	  	</modsTemplate>	</bibRefType>		<bibRefType name="proceedings paper">		<constraint require="title"/>		<constraint require="year" pattern="((1[5-9][0-9][0-9])|(20[0-9][0-9]))" message="Year of publication is missing or invalid."/>		<constraint require="volumeTitle" message="Volume title is missing."/>		<!--constraint condition="(not(starts-with(./@volumeTitle, 'Proc')))" message="Volume title does not match proceedings."/-->		<constraint attributes="volumeTitle" pattern="(((In(\s+(The|the))?)|(in(\s+(The|the))?)|The|the)\s+)?Proc(\.|eedings)\s+.*" message="Volume title does not match proceedings."/>		<constraint exclude="volume issue numero" message=">Proceedings papers don't have a volume, issue, or numero."/>		<constraint exclude="bookContentHint" message="Proceedings papers don't have book content hints."/>		<constraint attributes="pagination" pattern="([1-9][0-9]{0,4})(\s*\-+\s*[1-9][0-9]*)?"/>	  	<originTemplate>	  	</originTemplate>	  	<modsTemplate>	  	</modsTemplate>	</bibRefType>		<bibRefType name="journal volume">		<constraint require="year" pattern="((1[5-9][0-9][0-9])|(20[0-9][0-9]))" message="Year of publication is missing or invalid."/>		<constraint require="journalOrPublisher" message="Journal name is missing."/>		<constraint require="volume issue numero" pattern="[1-9][0-9]*" message="Volume, issue, and numero are missing or invalid."/>		<constraint exclude="pagination"/>		<constraint exclude="bookContentHint" message="Journal volumes don't have book content hints."/>	  	<originTemplate>	  	</originTemplate>		<modsTemplate>	  	</modsTemplate>	</bibRefType>		<bibRefType name="journal article">		<constraint require="title"/>		<constraint require="year" pattern="((1[5-9][0-9][0-9])|(20[0-9][0-9]))" message="Year of publication is missing or invalid."/>		<constraint require="journalOrPublisher" message="Journal name is missing."/>		<constraint require="volume issue numero" pattern="[1-9][0-9]*" message="Volume, issue, and numero are missing or invalid."/>		<constraint require="pagination" pattern="([1-9][0-9]{0,4})(\s*\-+\s*[1-9][0-9]*)?"/>		<constraint condition="(not(./@volumeTitle) and ./@editor)" message="Only special issues have editors."/>		<constraint exclude="bookContentHint" message="Journal articles don't have book content hints."/>	  	<originTemplate>	  	</originTemplate>	  	<modsTemplate>	  	</modsTemplate>	</bibRefType>		<bibRefType name="url" matchStrictness="4">		<constraint require="title"/>		<constraint require="url"/>		<constraint exclude="pagination" message="URLs don't have a pagination."/>		<constraint exclude="editor" message="URLs don't have editors."/>		<constraint exclude="volumeTitle" message="URLs don't have a volume title."/>		<constraint exclude="location" message="URLs don't have a location."/>		<constraint exclude="journal" message="URLs don't have a journal name."/>		<constraint exclude="volume issue numero" message=">URLs don't have a volume, issue, or numero."/>		<constraint exclude="bookContentHint" message="URLs don't have book content hints."/>	  	<originTemplate>	  	</originTemplate>	  	<modsTemplate>	  	</modsTemplate>	</bibRefType></bibRefTypeSystem>